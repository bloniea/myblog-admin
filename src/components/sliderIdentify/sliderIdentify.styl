.sliderIdentify {
  position relative
  width 278px
  height 30px
  box-sizing border-box
  .bg {
    width 100%
    height 100%
    background #fff
    border 1px solid #ccc
    line-height 30px
    text-align center
    font-size 14px
    position absolute
    user-select none
    border-radius 4px
  }
  .slider {
    position absolute
    cursor pointer
    border 1px solid #ccc
    // box-sizing border-box
    height 100%
    svg {
      width 14px
      height 14px
      color #fff
      padding 8px
      background #fff
    }
  }
  .mask {
    position absolute
    width 0
    height 100%
    background rgba(82, 19, 145, 0.41)
    /* transition:width 0.4s; */
  }
  .success {
    width 100%
    height 100%
    line-height 30px
    text-align center
    font-size 14px
    position absolute
    user-select none
    background #65e065
    color #fff
    display none
  }
}
