.avatar-uploader >>> .el-upload {
  border 1px dashed #d9d9d9
  border-radius 6px
  cursor pointer
  position relative
  overflow hidden
}
.avatar-uploader >>> .el-upload:hover {
  border-color #F284A9
}
.avatar-uploader-icon {
  font-size 28px
  color #8c939d
  width 178px
  height 178px
  line-height 178px
  text-align center
}
.preview {
  width 100%
  height auto
  display block
  margin-top 20px
}